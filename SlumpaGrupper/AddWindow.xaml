<Window x:Class="SlumpaGrupper.AddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SlumpaGrupper"
        mc:Ignorable="d"
        Title="Edit" Height="300" Width="300"
        Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" MinWidth="88"/>
            <ColumnDefinition Width="Auto" MinWidth="58.4"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0" Margin="10,10,10,10.4">
            <TextBox x:Name="StudentsTxtBox" AcceptsReturn="True" TextWrapping="Wrap" Margin="0,0,0,0.4" />
        </Grid>
        <Grid Grid.Column="1" Margin="0,10,9.4,10.4">
            <Button x:Name="SaveBtn" Content="Spara" HorizontalAlignment="Left" VerticalAlignment="Top" Width="49" Click="SaveBtn_Click" RenderTransformOrigin="-2.865,0.646"/>
        </Grid>

    </Grid>
</Window>
