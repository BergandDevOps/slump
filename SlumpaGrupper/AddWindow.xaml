<Window x:Class="SlumpaGrupper.AddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SlumpaGrupper"
        mc:Ignorable="d"
        Title="Edit" Height="300" Width="300"
        Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBox x:Name="StudentsTxtBox" Grid.Row="0" AcceptsReturn="True" TextWrapping="Wrap" />
        <Button x:Name="SaveBtn" Grid.Row="1" Content="Spara" HorizontalAlignment="Stretch" Click="SaveBtn_Click" Padding="5" />
    </Grid>
</Window>
